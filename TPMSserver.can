{
  byte dataBuffer[1];
  
  writeEx(0,1,"\nTPMS-SOME/IP: Preparing to send danger tyre pressure event." );
  
  // Insert danger limit value in the event message.
  
  dataBuffer[0]=@TPMS::DangerLimit;
    
  SomeIpSetData(messageHandle,1,dataBuffer);
  CheckForSomeIpError();

}


void OnPrepareEvent3(DWORD eventHandle, DWORD messageHandle)
{
  byte dataBuffer[1];
  
  writeEx(0,1,"\nTPMS-SOME/IP: Preparing to send danger limt VALUE event." );
  
  // Insert danger limit value in the event message.
  
  dataBuffer[0]=@TPMS::DangerZone;
    
  SomeIpSetData(messageHandle,1,dataBuffer);
